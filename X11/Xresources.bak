Xcursor.theme: Vimix-cursors
!Xcursor.size: 16

!!URxvt Appearance

!Font stuff
URxvt.font: xft:Hack:style=Regular:size=12
URxvt.boldFont: xft:Hack:style=Bold:size=12
URxvt.italicFont: xft:Hack:style=Italic:size=12
URxvt.boldItalicFont: xft:Hack:style=Bold Italic:size=12

!Other stuff
URxvt.scrollBar: false
URxvt.letterSpace: 0
URxvt.lineSpace: 0

!fake transparecy, without a compositor
!URxvt*transparent: true
!URxvt*shading: 7

!urxvt default size
!URxvt.geometry: 136x40

!!lwm
!lwm.button2: urxvt
